                },
                
                exportData: function() {
                    console.log('ğŸ“¤ å¯¼å‡ºæ•°æ®');
                    alert('ğŸ“Š æ•°æ®å¯¼å‡ºåŠŸèƒ½\næ­£åœ¨ç”ŸæˆExcelæŠ¥è¡¨...');
                },
                
                systemStatus: {
                    online: true,
                    lastUpdate: Date.now()
