use html5;

[Template] @Style DefaultText
{
    color: "black";
    font-size: "16px";
    line-height: "1.6";
}

html
{
    body
    {
        div
        {
            style
            {
                @Style DefaultText;
            }
            
            text { "使用样式模板的文本" }
        }
    }
}